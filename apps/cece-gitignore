#!/bin/bash
# CECE-GITIGNORE v1.0 - Gitignore Generator
python() {
    echo "__pycache__/
*.py[cod]
.env
venv/
.pytest_cache/"
}
node() {
    echo "node_modules/
.env
dist/
*.log"
}
general() {
    echo ".DS_Store
*.log
*.tmp
.env"
}
case "$1" in
    python) python ;;
    node) node ;;
    general) general ;;
    *) echo "CECE-GITIGNORE: python | node | general" ;;
esac
